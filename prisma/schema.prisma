generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./banco.db"
}


model users{
  id Int @id @default(autoincrement())
  username String @unique
  password String
  isAdmin Boolean @default(true)
}
model books{
  id Int @id @default(autoincrement())
  title String
  author String
  available Boolean @default(false)
}